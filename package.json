{
  "name": "tower-defense",
  "version": "0.0.1",
  "scripts": {
    "ng": "nx",
    "start": "ng serve",
    "start:dev": "npm-run-all -p start:ui-dev",
    "start:ui": "set NODE_OPTION=--openssl-legacy-provider && ng serve --proxy-config=proxy.conf.json",
    "start:ui-dev": "set NODE_OPTION=--openssl-legacy-provider && ng serve --proxy-config=proxy.conf.json --configuration=development",
    "build": "set NODE_OPTION=--openssl-legacy-provider && ng build",
    "test": "ng test",
    "lint": "eslint -c .eslintrc.js --ext .ts",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
